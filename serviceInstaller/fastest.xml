<?xml version="1.0" encoding="UTF-8"?>
<service>
    <id>fastest</id>
    <name>FasTest</name>
    <description>This service runs the FasTest Automation Testing Framework</description>
    <executable>java</executable>
    <arguments>-jar "D:\\fasTest\lib\fastest-automation-1.0.7-SNAPSHOT.jar" --server.port=8088 "D:\\fasTest\conf\fastest.properties"</arguments>
    <log mode="roll-by-size-time">
      <sizeThreshold>10240</sizeThreshold>
      <pattern>yyyyMMdd</pattern>
      <autoRollAtTime>00:00:00</autoRollAtTime>
      <zipOlderThanNumDays>90</zipOlderThanNumDays>
      <zipDateFormat>yyyyMM</zipDateFormat>
    </log>
    <logpath>D:\\fasTest\\log</logpath>
    <startmode>Automatic</startmode>                
</service>
